#include <physical_layouts.dtsi>

/ {
    skreecustom_physical_layout: skreecustom_physical_layout {
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";
        
        transform = <&default_transform>;
        
        keys  //                 w   h    x    y     rot  rx   ry
            // Row 0 (F-keys row)
            = <&key_physical_attrs 100 100    0    0    0    0    0>  // F1
            , <&key_physical_attrs 100 100  100    0    0    0    0>  // F2
            , <&key_physical_attrs 100 100  200    0    0    0    0>  // F3
            , <&key_physical_attrs 100 100  300    0    0    0    0>  // F4
            , <&key_physical_attrs 100 100  400    0    0    0    0>  // F5
            , <&key_physical_attrs 100 100  500    0    0    0    0>  // F6
            , <&key_physical_attrs 100 100  700    0    0    0    0>  // F7
            , <&key_physical_attrs 100 100  800    0    0    0    0>  // F8
            , <&key_physical_attrs 100 100  900    0    0    0    0>  // F9
            , <&key_physical_attrs 100 100 1000    0    0    0    0>  // F10
            , <&key_physical_attrs 100 100 1100    0    0    0    0>  // F11
            , <&key_physical_attrs 100 100 1200    0    0    0    0>  // F12
            
            // Row 1 (Number row)
            , <&key_physical_attrs 100 100    0  100    0    0    0>  // `
            , <&key_physical_attrs 100 100  100  100    0    0    0>  // 1
            , <&key_physical_attrs 100 100  200  100    0    0    0>  // 2
            , <&key_physical_attrs 100 100  300  100    0    0    0>  // 3
            , <&key_physical_attrs 100 100  400  100    0    0    0>  // 4
            , <&key_physical_attrs 100 100  500  100    0    0    0>  // 5
            , <&key_physical_attrs 100 100  700  100    0    0    0>  // 6
            , <&key_physical_attrs 100 100  800  100    0    0    0>  // 7
            , <&key_physical_attrs 100 100  900  100    0    0    0>  // 8
            , <&key_physical_attrs 100 100 1000  100    0    0    0>  // 9
            , <&key_physical_attrs 100 100 1100  100    0    0    0>  // 0
            , <&key_physical_attrs 100 100 1200  100    0    0    0>  // =
            
            // Row 2 (Top alpha row)
            , <&key_physical_attrs 100 100    0  200    0    0    0>  // Tab
            , <&key_physical_attrs 100 100  100  200    0    0    0>  // Q
            , <&key_physical_attrs 100 100  200  200    0    0    0>  // W
            , <&key_physical_attrs 100 100  300  200    0    0    0>  // E
            , <&key_physical_attrs 100 100  400  200    0    0    0>  // R
            , <&key_physical_attrs 100 100  500  200    0    0    0>  // T
            , <&key_physical_attrs 100 100  700  200    0    0    0>  // Y
            , <&key_physical_attrs 100 100  800  200    0    0    0>  // U
            , <&key_physical_attrs 100 100  900  200    0    0    0>  // I
            , <&key_physical_attrs 100 100 1000  200    0    0    0>  // O
            , <&key_physical_attrs 100 100 1100  200    0    0    0>  // P
            , <&key_physical_attrs 100 100 1200  200    0    0    0>  // -
            
            // Row 3 (Home row)
            , <&key_physical_attrs 100 100    0  300    0    0    0>  // Esc
            , <&key_physical_attrs 100 100  100  300    0    0    0>  // A
            , <&key_physical_attrs 100 100  200  300    0    0    0>  // S
            , <&key_physical_attrs 100 100  300  300    0    0    0>  // D
            , <&key_physical_attrs 100 100  400  300    0    0    0>  // F
            , <&key_physical_attrs 100 100  500  300    0    0    0>  // G
            , <&key_physical_attrs 100 100  700  300    0    0    0>  // H
            , <&key_physical_attrs 100 100  800  300    0    0    0>  // J
            , <&key_physical_attrs 100 100  900  300    0    0    0>  // K
            , <&key_physical_attrs 100 100 1000  300    0    0    0>  // L
            , <&key_physical_attrs 100 100 1100  300    0    0    0>  // ;
            , <&key_physical_attrs 100 100 1200  300    0    0    0>  // '
            
            // Row 4 (Bottom alpha row)
            , <&key_physical_attrs 100 100    0  400    0    0    0>  // LGui
            , <&key_physical_attrs 100 100  100  400    0    0    0>  // Z
            , <&key_physical_attrs 100 100  200  400    0    0    0>  // X
            , <&key_physical_attrs 100 100  300  400    0    0    0>  // C
            , <&key_physical_attrs 100 100  400  400    0    0    0>  // V
            , <&key_physical_attrs 100 100  500  400    0    0    0>  // B
            , <&key_physical_attrs 100 100  700  400    0    0    0>  // N
            , <&key_physical_attrs 100 100  800  400    0    0    0>  // M
            , <&key_physical_attrs 100 100  900  400    0    0    0>  // ,
            , <&key_physical_attrs 100 100 1000  400    0    0    0>  // .
            , <&key_physical_attrs 100 100 1100  400    0    0    0>  // /
            , <&key_physical_attrs 100 100 1200  400    0    0    0>  // \
            
            // Row 5 (Arrow keys)
            , <&key_physical_attrs 100 100  200  500    0    0    0>  // Up (left side)
            , <&key_physical_attrs 100 100  300  500    0    0    0>  // Down (left side)
            , <&key_physical_attrs 100 100  900  500    0    0    0>  // Left (right side)
            , <&key_physical_attrs 100 100 1000  500    0    0    0>  // Right (right side)
            
            // Thumb cluster - Left side (row 6, cols 0-2)
            , <&key_physical_attrs 100 100  100  600    0    0    0>  // [
            , <&key_physical_attrs 100 100  200  600    0    0    0>  // Enter
            , <&key_physical_attrs 100 100  300  600    0    0    0>  // Del
            
            // Thumb cluster - Right side (row 6, cols 8-6 in reverse)
            , <&key_physical_attrs 100 100  900  600    0    0    0>  // Bspc
            , <&key_physical_attrs 100 100  800  600    0    0    0>  // Space
            , <&key_physical_attrs 100 100  700  600    0    0    0>  // ]
            
            // Bottom thumb cluster - Left side (row 6, cols 3-5)
            , <&key_physical_attrs 100 100  100  700    0    0    0>  // PgUp
            , <&key_physical_attrs 100 100  200  700    0    0    0>  // MO(MOUSE)
            , <&key_physical_attrs 100 100  300  700    0    0    0>  // PrtSc
            
            // Bottom thumb cluster - Right side (row 6, cols 11-9 in reverse)
            , <&key_physical_attrs 100 100  900  700    0    0    0>  // Ins
            , <&key_physical_attrs 100 100  800  700    0    0    0>  // TOG(GAMING)
            , <&key_physical_attrs 100 100  700  700    0    0    0>  // PgDn
            ;
    };
};
